{
  "name": "transform-css",
  "version": "1.0.6",
  "description": "Transforms CSS into beautifully formatted styles ready to use with a preprocessor",
  "author": "Trevor Blades <tdblades@gmail.com>",
  "repository": "trevorblades/transform-css",
  "license": "MIT",
  "keywords": [
    "css",
    "less",
    "sass",
    "scss",
    "stylus",
    "styles",
    "transform",
    "stylesheet",
    "preprocessor"
  ],
  "main": "lib/index.js",
  "module": "index.js",
  "scripts": {
    "pretest": "eslint index.js test.js",
    "test": "jest --env=node",
    "prepare": "babel index.js --out-dir lib --presets=@babel/env"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@trevorblades/eslint-config": "^6.17.0",
    "eslint": "^5.2.0",
    "outdent": "^0.5.0"
  },
  "dependencies": {
    "css": "^2.2.3",
    "indent-string": "^3.2.0",
    "jest": "^23.4.2",
    "lodash": "^4.17.10"
  }
}
